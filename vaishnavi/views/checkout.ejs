<title>
    cashfree collect payments
</title>
<body>
    <form id="checkoutForm" name="checkoutForm" method="POST" action= <%= postUrl %>>
        
    <table class="mainForm">
    <tr>
        <td>order id</td>
        <td><input type="text" name="orderId"  ></td>
    </tr>
    <tr>
        <td>order amount</td>
        <td><input type="text" name="orderAmount"  ></td>
    </tr>
    <tr>
        <td>customer name</td>
        <td><input type="text" name="customerName" value="<%=user.name %>" ></td>
    </tr>
    <tr>
        <td>customer email</td>
        <td><input type="text" name="customerEmail" value="<%= user.email %>" ></td>
    </tr>
    <tr>
        <td>customer phone</td>
        <td><input type="text" name="customerPhone" value="<%= user.phNum %>" ></td>
        </tr>
</table>
        <table>
        <tr>
            <td colspan="2"><input type="submit" value="pay cashfree" id="payBtn" style="display:none" ></td>
        </tr>
        <tr>
            <td colspan="2"><button value="pay" id="submitBtn">pay</button></td>
        </tr>
        </table>
    </form>
    <div id="generatedSignature"></div>
</body>
